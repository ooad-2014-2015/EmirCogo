<UserControl x:Class="eRestoran.JeloKontrola"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot" Background="{StaticResource PhoneChromeBrush}">
        <StackPanel HorizontalAlignment="Left" Height="460" Margin="10,10,0,0" VerticalAlignment="Top" Width="460">
            <TextBlock x:Name="ImeText" TextWrapping="Wrap" Text="TextBlock" Margin="0,0,177,0"/>
            <Image x:Name="slikaKontrola" HorizontalAlignment="Left" Height="100" Margin="361,19,0,0" VerticalAlignment="Top" Width="100"/>
            <TextBlock x:Name="OpisText" TextWrapping="Wrap" Text=""/>
        </StackPanel>
        
    </Grid>
</UserControl>